<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Basic blocks for building algorithms and data structures.
Commonly used types:
• Endpoints
• ArrayPrefix&lt;T&gt;
• ArrayPrefixEnumerator&lt;T&gt;</Description>
    <PackageTags>graph;primitive</PackageTags>
    <Title>Stable primitive types</Title>
    <VersionPrefix>0.4.0</VersionPrefix>
  </PropertyGroup>
  <PropertyGroup>
    <AssemblyVersion>0.4</AssemblyVersion>
    <FileVersion>$(VersionPrefix)</FileVersion>
    <LangVersion>8</LangVersion>
    <TargetFrameworks>netcoreapp2.0;net461;netstandard1.3;netstandard2.0;netstandard2.1</TargetFrameworks>
  </PropertyGroup>
  <PropertyGroup Condition="'$(DebugType)' == 'portable'">
    <EmbedAllSources>true</EmbedAllSources>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="3.3.0" PrivateAssets="all" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="all" />
  </ItemGroup>
  <Import Project="..\Arborescence.Helpers\Arborescence.Helpers.projitems" Label="Shared" />
</Project>
