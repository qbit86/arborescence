<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Basic blocks for building algorithms and data structures.
Commonly used types:
• Endpoints&lt;TVertex&gt;
• ArrayPrefix&lt;T&gt;</Description>
    <PackageTags>graph;primitive</PackageTags>
    <Title>Stable primitive types</Title>
    <VersionPrefix>0.2.1</VersionPrefix>
    <VersionSuffix>preview</VersionSuffix>
  </PropertyGroup>
  <PropertyGroup>
    <AssemblyVersion>0.2</AssemblyVersion>
    <FileVersion>$(VersionPrefix)</FileVersion>
    <TargetFrameworks>net461;netstandard1.0;netstandard2.0</TargetFrameworks>
  </PropertyGroup>
  <PropertyGroup Condition="'$(DebugType)' == 'portable'">
    <EmbedAllSources>true</EmbedAllSources>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="3.0.0" PrivateAssets="all" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="all" />
  </ItemGroup>
  <Import Project="..\Arborescence.Helpers\Arborescence.Helpers.projitems" Label="Shared" />
</Project>
