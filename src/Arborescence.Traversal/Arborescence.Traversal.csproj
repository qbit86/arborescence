<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Graph traversal algorithms: DFS, BFS.
Commonly used types:
• Bfs&lt;TGraph, TEdge, TEdgeEnumerator&gt;
• Dfs&lt;TGraph, TEdge, TEdgeEnumerator&gt;
• InstantDfs&lt;TGraph, TVertex, TEdge, TEdgeEnumerator, TColorMap, TGraphPolicy, TColorMapPolicy&gt;
• IDfsHandler&lt;TGraph, TVertex, TEdge&gt;</Description>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <PackageTags>graph;traversal;bfs;dfs</PackageTags>
    <Title>Graph traversal algorithms</Title>
    <VersionPrefix>0.5.0</VersionPrefix>
  </PropertyGroup>
  <PropertyGroup>
    <AssemblyVersion>0.5</AssemblyVersion>
    <FileVersion>$(VersionPrefix)</FileVersion>
    <RootNamespace>Arborescence.Traversal</RootNamespace>
    <TargetFrameworks>netcoreapp2.0;net461;netstandard1.3;netstandard2.0;netstandard2.1</TargetFrameworks>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Arborescence.Abstractions\Arborescence.Abstractions.csproj" />
  </ItemGroup>
  <Import Project="..\Arborescence.Internal\Arborescence.Internal.projitems" Label="Shared" />
  <ItemGroup Condition="'$(TargetFramework)' == 'net461' OR '$(TargetFramework)' == 'netstandard1.3' OR '$(TargetFramework)' == 'netstandard2.0'">
    <PackageReference Include="System.Buffers" Version="4.5.1" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="3.3.1" PrivateAssets="all" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="all" />
  </ItemGroup>
</Project>
