<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Graph traversal algorithms: DFS, BFS.
Commonly used types:
InstantDfs&lt;TGraph, TVertex, TEdge, TEdgeEnumerator, TColorMap, TGraphPolicy, TColorMapPolicy&gt;
EnumerableBfs&lt;TGraph, TVertex, TEdge, TEdgeEnumerator, TExploredSet, TGraphPolicy, TExploredSetPolicy&gt;</Description>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <PackageTags>graph;traversal;dfs;bfs</PackageTags>
    <Title>Graph traversal algorithms</Title>
    <VersionPrefix>0.1.1</VersionPrefix>
    <VersionSuffix>preview</VersionSuffix>
  </PropertyGroup>
  <PropertyGroup>
    <AssemblyVersion>0.1</AssemblyVersion>
    <FileVersion>$(VersionPrefix)</FileVersion>
    <NoWarn Condition="'$(Configuration)' != 'Release'">$(NoWarn);1591</NoWarn>
    <RootNamespace>Arborescence.Traversal</RootNamespace>
    <TargetFrameworks>net461;netstandard1.3;netstandard2.0;netstandard2.1;netcoreapp2.0;netcoreapp3.0</TargetFrameworks>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Arborescence.Abstractions\Arborescence.Abstractions.csproj" />
  </ItemGroup>
  <Import Project="..\Arborescence.Internal\Arborescence.Internal.projitems" Label="Shared" />
  <ItemGroup Condition="'$(TargetFramework)' == 'net461' OR '$(TargetFramework)' == 'netstandard1.3' OR '$(TargetFramework)' == 'netstandard2.0'">
    <PackageReference Include="System.Buffers" Version="4.5.1" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="3.0.0" PrivateAssets="all" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="all" />
  </ItemGroup>
</Project>
