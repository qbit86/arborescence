<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Data structures for graphs and some specialized collections.
Commonly used types:
• IndexedIncidenceGraph
• SimpleIncidenceGraph
• IGraphBuilder&lt;TGraph, TVertex, TEdge&gt;</Description>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageTags>graph;model;adjacency</PackageTags>
    <Title>Graph models</Title>
    <VersionSuffix>preview</VersionSuffix>
  </PropertyGroup>
  <PropertyGroup>
    <AssemblyVersion>0.14</AssemblyVersion>
    <DebugType>portable</DebugType>
    <FileVersion>$(VersionPrefix)</FileVersion>
    <GenerateCompatibilitySuppressionFile>true</GenerateCompatibilitySuppressionFile>
    <RootNamespace>Arborescence.Models</RootNamespace>
    <TargetFrameworks>net461;net6.0;net7.0;netcoreapp3.1;netstandard1.3;netstandard2.0;netstandard2.1</TargetFrameworks>
  </PropertyGroup>
  <PropertyGroup Condition="'$(TargetFramework)' == 'net7.0'">
    <LangVersion>11</LangVersion>
  </PropertyGroup>
  <ItemGroup>
    <None Include="README.md" Pack="true" PackagePath="/" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="DotNet.ReproducibleBuilds" PrivateAssets="All" />
  </ItemGroup>
  <ItemGroup Condition="'$(TargetFramework)' == 'net461' OR '$(TargetFramework)' == 'netstandard1.3' OR '$(TargetFramework)' == 'netstandard2.0'">
    <PackageReference Include="System.Buffers" />
    <PackageReference Include="System.Memory" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Arborescence.Abstractions\Arborescence.Abstractions.csproj" />
    <ProjectReference Include="..\Arborescence.Primitives\Arborescence.Primitives.csproj" />
  </ItemGroup>
  <Import Project="..\Arborescence.Shared\Arborescence.ArrayPrefixBuilder\Arborescence.ArrayPrefixBuilder.projitems" Label="Shared" />
  <Import Project="..\Arborescence.Shared\Arborescence.TryHelpers\Arborescence.TryHelpers.projitems" Label="Shared" />
  <Import Condition="'$(TargetFramework)' == 'net461' OR '$(TargetFramework)' == 'netstandard1.3' OR '$(TargetFramework)' == 'netstandard2.0'" Project="..\Arborescence.Shared\Arborescence.NullableAttributes\Arborescence.NullableAttributes.projitems" Label="Shared" />
</Project>
