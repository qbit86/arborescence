<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net461;netstandard1.1;netstandard2.0</TargetFrameworks>
    <NeutralLanguage>en-US</NeutralLanguage>
    <AssemblyVersion>0.1</AssemblyVersion>
    <VersionPrefix>0.1.0</VersionPrefix>
    <FileVersion>$(VersionPrefix)</FileVersion>
    <Authors>Viktor T.</Authors>
    <Description>Simple parsers and test resources.</Description>
    <PackageLicenseUrl>https://github.com/qbit86/ubiquitous-graphs/blob/master/LICENSE.txt</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/qbit86/ubiquitous-graphs</RepositoryUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <PackageTags>graph;graphviz;dot</PackageTags>
    <PackageIconUrl>https://github.com/qbit86/ubiquitous-graphs/raw/master/kopimi.png</PackageIconUrl>
    <SignAssembly>true</SignAssembly>
    <RootNamespace>Ubiquitous.Workbench</RootNamespace>
    <LangVersion>7.2</LangVersion>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="IndexedGraphs\*.gv" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="IndexedGraphs\*.gv" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Ubiquitous.Primitives\Ubiquitous.Primitives.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CodeQuality.Analyzers" Version="2.6.3" />
    <PackageReference Include="System.Memory" Version="4.5.2" />
    <PackageReference Include="Text.Analyzers" Version="2.6.3" />
  </ItemGroup>
</Project>
