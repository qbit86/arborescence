<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net461;netstandard1.0;netstandard2.0</TargetFrameworks>
    <NeutralLanguage>en-US</NeutralLanguage>
    <AssemblyVersion>0.1</AssemblyVersion>
    <VersionPrefix>0.1.0</VersionPrefix>
    <FileVersion>$(VersionPrefix)</FileVersion>
    <Authors>Viktor T.</Authors>
    <Title>Stable primitive types</Title>
    <Description>Low-level building blocks used in public APIs.
Commonly used types: SourceTargetPair&lt;TVertex&gt;, ArrayPrefix&lt;T&gt;.</Description>
    <PackageLicenseUrl>https://github.com/qbit86/ubiquitous-graphs/blob/master/LICENSE.txt</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/qbit86/ubiquitous-graphs</RepositoryUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <PackageTags>graph;primitive</PackageTags>
    <PackageIconUrl>https://github.com/qbit86/ubiquitous-graphs/raw/master/kopimi.png</PackageIconUrl>
    <PackageReleaseNotes>https://github.com/qbit86/ubiquitous-graphs/blob/develop/src/Ubiquitous.Primitives/CHANGELOG.md</PackageReleaseNotes>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>$(UbiquitousRootDir)ubiquitous-graphs.snk</AssemblyOriginatorKeyFile>
    <RootNamespace>Ubiquitous</RootNamespace>
    <LangVersion>7.2</LangVersion>
  </PropertyGroup>
  <PropertyGroup Condition="'$(DebugType)' == 'portable'">
    <EmbedAllSources>true</EmbedAllSources>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>
  <Import Project="..\Ubiquitous.Helpers\Ubiquitous.Helpers.projitems" Label="Shared" />
</Project>
