<Project>
  <PropertyGroup>
    <LangVersion>7.2</LangVersion>
    <NeutralLanguage>en-US</NeutralLanguage>
    <RepoRoot>$(MSBuildThisFileDirectory)</RepoRoot>
    <RootNamespace>Ubiquitous</RootNamespace>
  </PropertyGroup>
  <PropertyGroup>
    <AssemblyOriginatorKeyFile>$(RepoRoot)ubiquitous-graphs.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>
  <PropertyGroup>
    <Authors>Viktor T.</Authors>
    <PackageIconUrl>https://github.com/qbit86/ubiquitous-graphs/raw/master/kopimi.png</PackageIconUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/qbit86/ubiquitous-graphs</RepositoryUrl>
  </PropertyGroup>
</Project>
